{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --production=false",
  "functions": {
    "api/events.ts": {
      "runtime": "nodejs18.x"
    },
    "api/migrate.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/events.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
